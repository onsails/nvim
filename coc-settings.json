{
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  },
  "coc.preferences.formatOnSaveFiletypes": ["vim", "rust", "json"],
